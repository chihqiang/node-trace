{
  "name": "@chihqiang/node-trace",
  "version": "0.1.0",
  "description": "A lightweight event tracking library for Node.js and browsers, with device ID generation, browser data collection, and plugin system",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./global": {
      "import": "./dist/index.global.js",
      "require": "./dist/index.global.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "test": "jest --detectOpenHandles",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "format": "prettier --write src/**/*.ts",
    "dev": "tsup --watch",
    "prepublishOnly": "run-s format test build"
  },
  "keywords": [
    "event-tracking",
    "analytics",
    "device-id",
    "browser-data",
    "performance-monitoring",
    "user-behavior",
    "pageview",
    "network-monitoring",
    "plugin-system",
    "typescript"
  ],
  "author": "zhiqiang",
  "license": "Apache-2.0",
  "packageManager": "pnpm@10.23.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chihqiang/node-trace.git"
  },
  "homepage": "https://github.com/chihqiang/node-trace#readme",
  "bugs": {
    "url": "https://github.com/chihqiang/node-trace/issues"
  },
  "files": [
    "dist",
    "README.md",
    "package.json"
  ],
  "engines": {
    "node": ">=14.0.0"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^25.1.0",
    "jest": "^30.2.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.8.1",
    "ts-jest": "^29.4.6",
    "ts-node": "^10.9.2",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "dexie": "^4.3.0"
  }
}
